{
  "version": "1.0.0-*",
  "testRunner": "nunit",
  "dependencies": {
    "dotnet-test-nunit": "3.4.0-beta-2",
    "Nancy.Testing": "2.0.0-barneyrubble",
    "NUnit": "3.4.1",
    "paramore.brighter.commandprocessor": {
      "target": "project"
    },
    "paramore.brighter.commandprocessor.messageviewer": {
      "target": "project"
    },
    "paramore.brighter.commandprocessor.tests.nunit": {
      "target": "project"
    },
    "NUnit.Specifications.NUnit3.coreCLR": {
      "target": "project"
    },
    "nUnitShouldAdapter": {
      "target": "project"
    }
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      }
    },
    "net46": {
      "dependencies": {
      }
    }
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true
      }
    }
  }
}
